<view class='box'>
  <view class='nav' wx:for="{{order}}">
    <view class='nav-top'>
      <view class='nav-left'>订单号:{{order[index].orderID}}</view>
      <view class='nav-right'>{{order[index].status}}</view>
    </view>
    <view class='nav-section'>
      <view class='section-left'>
        <image src='{{order[index].images}}'></image>
      </view>
      <view class='section-right'>
        <view class='temo'>
            <view class='trmo-top'>{{order[index].name}}</view>
            <view class='trmo-bottom'>{{}}</view>
        </view>
      </view>
    </view>
 <view class='lisr-footer'>
      <view class='lisr-footer-left'>{{order[index].endData}}</view>
      <view class='lisr-footer-right'>共{{order[index].cont}}件商品,实付￥{{order[index].money}}</view>
    </view>
  </view>
</view>